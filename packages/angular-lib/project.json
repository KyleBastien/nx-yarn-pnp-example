{
  "name": "angular-lib",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/angular-lib/src",
  "prefix": "nx-yarn-pnp-example",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/angular-lib"],
      "options": {
        "jestConfig": "packages/angular-lib/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/angular-lib/src/**/*.ts",
          "packages/angular-lib/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
